{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch tlgconv",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tlgconv",
            "args": [
                "test/images/dko_sss.bmp",
                "/tmp/dko_sss.tlg8",
                "--tlg-version=8"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cmake build"
        }
    ]
}